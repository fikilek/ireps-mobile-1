        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.smars.irepsmobile1"
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "e59033ef-5046-42da-8e67-d010def0eabb"
      }
    },
    "plugins": [
      "expo-router"
    ]
  }
